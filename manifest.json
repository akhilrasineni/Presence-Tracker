
{
  "manifest_version": 3,
  "name": "RASINENI PT PRO",
  "version": "2.0.0",
  "description": "Professional presence tracking & Neural Web Assistant.",
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "activeTab",
    "scripting"
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "RASINENI PT PRO",
    "default_width": 450,
    "default_height": 600
  }
}
